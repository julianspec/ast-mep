		"@angular-material-components/datetime-picker": "^2.0.4",
		"@angular-material-components/moment-adapter": "^2.0.2",
		"@angular/animations": "~9.1.11",
		"@angular/cdk": "^9.2.4",
		"@angular/common": "~9.1.11",
		"@angular/compiler": "~9.1.11",
		"@angular/core": "~9.1.11",
		"@angular/forms": "~9.1.11",
		"@angular/material": "^9.2.4",
		"@angular/material-moment-adapter": "^8.2.3",
		"@angular/platform-browser": "~9.1.11",
		"@angular/platform-browser-dynamic": "~9.1.11",
		"@angular/router": "~9.1.11",
		"@dicebear/avatars": "^4.4.1",
		"@dicebear/avatars-initials-sprites": "^4.4.1",
		"@ngx-translate/core": "^12.1.2",
		"@ngx-translate/http-loader": "^4.0.0",
		"angular-oauth2-oidc": "^9.2.2",
		"moment": "^2.26.0",
		"ng-mat-select-infinite-scroll": "^2.0.0",
		"ngx-material-file-input": "^2.1.1",
		"rxjs": "~6.5.4",
		"tslib": "^1.10.0",
		"zone.js": "~0.10.2"
