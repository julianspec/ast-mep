{
	"name": "MEP",
	"version": "0.0.1",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --port 4200 --open --proxy-config proxy.conf.json",
		"start:mock": "ng serve --port 4200 --open --proxy-config proxy.conf.mock.json",
		"init": "npm i && npm start",
		"build": "node find-zones.js && ng build --prod",
		"test": "ng test",
		"test:coverage": "ng test --code-coverage ",
		"test:open": "bash testcoverage.sh",
		"lint": "ng lint",
		"sonar:testcoverage": "npm i && ng test --watch=false --code-coverage && sonar-scanner",
		"sonar": "npm i && sonar-scanner",
		"doc": "compodoc -p tsconfig.json -s",
		"deploy:dev": "bash ci.sh",
		"e2e": "ng e2e",
		"find-uw-zones": "node find-zones.js"
	},
	"private": true,
	"dependencies": {
		"@accusys/uw-core-authentication": "0.0.6",
		"@accusys/uw-core-components": "0.0.7",
